{
  "name": "symposium",
  "displayName": "Symposium",
  "description": "AI the Rust Way",
  "version": "1.3.0",
  "publisher": "symposium-dev",
  "license": "MIT OR Apache-2.0",
  "icon": "resources/extension-icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/symposium-dev/symposium"
  },
  "engines": {
    "vscode": "^1.104.0"
  },
  "categories": [
    "AI",
    "Chat"
  ],
  "main": "./out/extension.js",
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "viewsContainers": {
      "panel": [
        {
          "id": "symposium-panel",
          "title": "Symposium",
          "icon": "resources/icon.svg"
        }
      ]
    },
    "views": {
      "symposium-panel": [
        {
          "type": "webview",
          "id": "symposium.toadView",
          "name": "Symposium"
        }
      ]
    },
    "commands": [
      {
        "command": "symposium.openChat",
        "title": "Open Symposium"
      }
    ],
    "keybindings": [
      {
        "command": "symposium.openChat",
        "key": "alt+s alt+s",
        "mac": "alt+s alt+s"
      }
    ],
    "configuration": {
      "title": "Symposium",
      "properties": {
        "symposium.logLevel": {
          "type": "string",
          "default": "error",
          "enum": [
            "error",
            "info",
            "debug"
          ],
          "description": "Log level for the Symposium extension output channel."
        },
        "symposium.acpAgentPath": {
          "type": "string",
          "default": "",
          "description": "Override path to the symposium-acp-agent binary."
        },
        "symposium.toadCommand": {
          "type": "string",
          "default": "toad",
          "description": "Path to the toad binary."
        },
        "symposium.toadPort": {
          "type": "number",
          "default": 0,
          "description": "Port for the Toad web server. 0 means auto-select a free port."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run webpack",
    "webpack": "webpack --mode production",
    "webpack-dev": "webpack --mode development",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/vscode": "^1.104.0",
    "ts-loader": "^9.5.0",
    "typescript": "^5.3.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  }
}
